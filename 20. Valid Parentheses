class Solution:
    def isValid(self, s: str) -> bool:
        openmap = {'(':True, '{':True, '[':True}
        match = {'(':')', '{':'}', '[':']'}
        mystack = []
        for l in s:
            #print(l, mystack)
            if l in openmap:
                mystack.append(l)
            else:
                if len(mystack) == 0 or l != match[mystack.pop()]:
                    return False
        return len(mystack) == 0
